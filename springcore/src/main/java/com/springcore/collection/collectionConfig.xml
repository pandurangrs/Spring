<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!--2 COLLECTION TYPE -->

	<bean class="com.springcore.collection.Emp" id="emp1"
		p:name="Pandurang">

		<property name="phones">
			<list>
				<value>7083021253</value>
				<value>8766789525</value>
				<null/>
			</list>
		</property>
		<property name="Address">
			<set>
				<value>Nashik</value>
				<value>Nagar</value>
			</set>
		</property>

		<property name="course">
			<map>
				<entry key="Java" value="1-2 month"/>
				<entry key="Core Java" value="3-4 month"/>
			</map>
		</property>
		
		
		<property name="props">
			<props>
				<prop key="1">Pandurang</prop>
				<prop key="2">Hari</prop>
				<prop key="3">Vasudev</prop>
				<prop key="4">Hari</prop>
			</props>
		</property>
	</bean>

	


</beans>